<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata" fqn="DisplayContact">
    <apiVersion>52.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
    <target>lightning__AppPage</target>
    <target>lightning__RecordPage</target>
    <target>lightning__HomePage</target>
   
    <target>lightning__RecordAction</target>
    <target>lightning__FlowScreen</target>
  </targets>
  <targetConfigs>
    <targetConfig targets="lightning__FlowScreen">
        <property
                name="recordId"
                type="String"
                label="emailto"
                description="Pass the page's record id to the component variable"
                default="{!recordId}" />
                <property name="outputTest" type="String" role="outputOnly"/>
                <property name="last_name" type="String" label="Last Name" description="Last Name" default="{!last_name}"/>  
                <property name="objectApiName" type="String" label="objectApiName" description="Last Name" default="{!objectApiName}"/>  
                
            </targetConfig>
</targetConfigs>

</LightningComponentBundle>